#include<Servo.h>
Servo servoMotor1; 
Servo servoMotor2;
int pos=0;
void setup()
{
servoMotor1.attach(3); 
servoMotor2.attach(5);
}
void loop()
{
for(pos=0;pos<=180;pos+=1)
{
servoMotor1.write(pos);  
servoMotor2.write(pos); 
delay(15);
}
for(pos=180;pos>=0;pos-=1)
{
servoMotor1.write(pos);
servoMotor1.write(pos); 
delay(15);
}
} 

